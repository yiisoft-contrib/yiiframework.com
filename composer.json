        "yiisoft/yii2-elasticsearch": "~2.1.0",
        "yiisoft/yii2-redis": "~2.0.0",
        "yiisoft/yii2-queue": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "cebe/yii2-gravatar": "1.1",
        "creocoder/yii2-flysystem": "~0.9.0",

