    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "roave/security-advisories": "dev-master",

        "yiisoft/yii2": "~2.0.36",

        "yiisoft/yii2-apidoc": "~2.1.4@dev",
        "cebe/markdown-latex": "^1.1.3@dev",
        "cebe/markdown": "~1.0.3",

